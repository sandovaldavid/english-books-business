# Pageclip API key for form submissions
PAGECLIP_KEY=your_pageclip_key_here

# reCAPTCHA configuration
RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# Set to 'false' to disable reCAPTCHA (useful for development)
ENABLE_RECAPTCHA=true

# Set to 'true' to enable development mode (no reCAPTCHA on localhost)
IS_DEVELOPMENT=true
